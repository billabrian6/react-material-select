    "dev": "webpack-dev-server --history-api-fallback --hot --inline --progress",
    "build": "rimraf dist && webpack --config webpack.build.js --bail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leshek-pawlak/react-material-select"
  },
  "homepage": "http://react-material-select.netlify.com/",
